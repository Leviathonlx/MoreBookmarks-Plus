				name = _show
				duration = 0.7
				alpha = 1.0
			}

			state = {
				name = _hide
				alpha = 0.0
			}
		}

		highlight_icon = {
			visible = "[DomicileWindow.ShowNewBuildingEffect( DomicileBuildingSlot.Self )]"
			size = { 100% 100% }
			position = { 0 0 }
			datacontext = "[DomicileBuildingSlot.GetAsset]"
